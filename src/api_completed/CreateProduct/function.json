{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "product"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputProduct",
      "databaseName": "maindb",
      "collectionName": "data",
      "createIfNotExists": "false",
      "connectionStringSetting": "CONNECTION_STRING",
      "partitionKey": "product"
    }
  ],
  "scriptFile": "../dist/CreateProduct/index.js"
}
